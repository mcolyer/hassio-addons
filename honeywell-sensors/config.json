{
  "name": "Honeywell Sensors",
  "arch": ["amd64"],
  "version": "0.9",
  "slug": "honeywell-sensors",
  "image": "mcolyer/hassio-{arch}-honeywell-sensors",
  "description": "345MHz RF to MQTT Bridge based on RTL_SDR for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/mcolyer/hassio-addons/tree/master/honeywell-sensors",
  "startup": "before",
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "host_network": "False",
  "options":
  {
    "mqtt_host": "hassio.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "homeassistant/sensor/honeywell"
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str"
   }
}
